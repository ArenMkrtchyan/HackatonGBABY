@model List<HackatonGBABY.Models.Resualt>
<br />
@{
    Html.RenderPartial("Index");
}
<ul>
    @foreach (var item in Model)
    {
        <li style="width:500px">
            @if (item.IsValid)
            {
                <a href="@item.Link" style="color:blue">@item.Title</a>

                <br />
                <span style="font-size:15px;color:green">@item.Link</span>

                <br />
                <span style="color:gray;">@item.Snippet</span>
            }
            else
            {
                <a href="@item.Link" style="color:red">@item.Title</a> <br />
                <span style="font-size:15px;color:red">@item.Link</span>

                <br />
                <span style="color:red;">@item.Snippet</span>
            }


        </li>
    }
</ul>